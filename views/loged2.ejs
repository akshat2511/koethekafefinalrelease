  <section class="section countdown bg-black-10 text-center" aria-label="countdown" id="workshop">
    <div class="container">

      <p class="section-subtitle label-2">WELCOME</p>

      <h2 class="headline-1 section-title">BOOKING DETAILS</h2>
      <center>
        <div style="overflow-x: auto;">
          <table class="table table-striped table-bordered border-white"
            style="color:antiquewhite; border: 10px; border-color: brown;">
            <thead>
              <tr>
                <th scope="col">Date</th>
                <th scope="col">Time</th>
                <th scope="col">Name</th>
                <th scope="col">Phone No.</th>
                <th scope="col">No. of people</th>
                <th scope="col">Message</th>

              </tr>
            </thead>
            <tbody>
              <%m.forEach((n)=>{%>


                <tr>
                  <th scope="row">
                    <%=n.date%>
                  </th>
                  <td>
                    <%=n.time%>
                  </td>
                  <td>
                    <%=n.name%>
                  </td>
                  <td>
                    <%=n.phone%>
                  </td>
                  <td>
                    <%=n.person%>
                  </td>
                  <td>
                    <%=n.message%>
                  </td>

                </tr>
                <% })%>
            </tbody>
          </table>
        </div>
        <form id="signupForm" class="signup-form" action="/book" method="post">
          <label for="name">Enter the Name of Customer:</label>
          <input type="text" id="name" name="n1" required>
          <br><button type="submit" class="btn btn-primary">
            <span class="text text-1">DELETE</span>
            <span class="text text-2" aria-hidden="true">DELETE</span>
          </button>
        </form>

      </center>






    </div>
  </section>
